{
  "expo": {
    "name": "DONWORRY",
    "slug": "client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo/donLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/logo/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidNavigationBar": {
      "backgroundColor": "#FFFFFF",
      "barStyle": "dark-content"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "NOTIFICATIONS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo/donLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.honggh.client"
    },
    "web": {
      "favicon": "./src/assets/logo/donLogo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "06f09189-4993-4a5b-883f-13fc0140ea50"
      }
    }
  }
}

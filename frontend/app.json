{
  "expo": {
    "name": "client",
    "slug": "client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo/donLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/logo/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidNavigationBar": {
      "backgroundColor": "#FFFFFF",
      "barStyle": "dark-content"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo/donLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.honggh.client"
    },
    "web": {
      "favicon": "./src/assets/logo/donLogo.png"
    },
    "plugins": [
      "expo-router"
    ]
   
  }
}
